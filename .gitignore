# Ignore directories generated by Composer.
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/
/web/themes/custom/*/libraries/
/web/themes/custom/*/css/

# Ignore directories and files generated by npm/yarn.
node_modules
yarn-error.log

# Ignore directories and files generated by front-end build scripts.
web/themes/custom/benzy/js/build/
web/sites/**/themes/**/js/build/

# Ignore drupal-scaffold core files that haven't been customized. To customize
# one, remove it from this list and commit the file with the needed changes. It
# may also be desirable to then ignore it within drupal-scaffold configuration.
/web/.csslintrc
/web/.editorconfig
/web/.eslintignore
/web/.eslintrc.json
/web/.gitattributes
/web/.gitignore
/web/.htaccess
/web/autoload.php
/web/index.php
/web/robots.txt
/web/sites/default/default.settings.php
/web/sites/default/default.services.yml
/web/sites/development.services.yml
/web/sites/local.services.yml
/web/sites/example.sites.php
/web/update.php
/web/web.config
/web/INSTALL.txt
/web/README.txt
/web/example.gitignore
/web/themes/.gitignore
/web/sites/.gitignore
/web/profiles/.gitignore
/web/modules/.gitignore

# Ignore sensitive information.
/web/sites/sites.local.php
/web/sites/*/settings.local.php

# Ignore Drupal's file directory.
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore compiled CSS files.
/web/sites/*/themes/*/css

# Ignore stylelint config.
/web/sites/*/themes/*/.stylelintrc

# Ignore generated PHPUnit files
/tests/phpunit

# Ignore files generated by IDEs and operating systems.
/.idea/
.DS_Store

# Ignore sitemap.xml.
/web/sitemap.xml

# Ignore the Drupal Console directory.
/console

# Ignore test suite .env file.
/tests/.env

# Ignore test result output file.
/web/test-results.txt

# Ignore db dumps.
*-latest.sql.gz
/.editorconfig
/.gitattributes

# Ignore generated files from local development tooling
/web/sites/fe.development.services.yml

# Ignore files generated by VSCode..
/.vscode/
